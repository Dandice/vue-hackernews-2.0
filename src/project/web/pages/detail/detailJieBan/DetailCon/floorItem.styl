.fir_flo_together {
    margin: 20px 0;
    border: 1px solid #ececec;
    border-bottom: none;
    font-size: 14px;
    color: #636363;
    padding: 18px 25px;
    overflow: hidden;
    background: url(//common1.qyerstatic.com/bbs/old/images/xplanbtm.png) 0 100% repeat-x;
    .title {
        padding: 0 12px;
        background-color: #f5f5f5;
        line-height: 44px;
        border-radius: 3px;
    }
    .key {
        color: #959595;
        width: 115px;
    }
    .com_row {
        display flex;
        p {
            margin: 0;
        }
        .left {
            width: 14px;
            height 14px;
            padding-right 10px;
        }
        .right {
            color: rgba(0, 0, 0, .8);
            em {
                color: #999;
                font-style: normal;
            }
        }
    }
    .row {
        width: 705px;
        margin-top: 24px;
        padding-bottom: 22px;
        line-height: 20px;
        border-bottom: 1px dashed #ececec;
    }
    .row_two {
        margin-top: 24px;
        padding-bottom: 22px;
    }
    .tog_detail {
        width: 590px;
        img {
            max-width 100%;
        }
    }
}

#app .floor_item {
    padding: 20px 15px 15px 0;
    h1, h2 {
        margin: 22px 0;
        font-weight bold;
    }
    h1 {
        font-size 24px;
    }
    h2 {
        font-size 18px;
    }
    .placeholder-content {
        height: 68px;
        overflow: hidden;
        position: relative;
        animation: placeholderAnimate 2s infinite linear forwards;
        background: #f6f7f8; // Fallback
        background: linear-gradient(to right, #eee 2%, #ddd 18%, #eee 33%);
        margin: 30px 0;
        &_item {
            width: 100%;
            height: 20px;
            position: absolute;
            background: #fff;

            &:after,
            &:before {
                width: inherit;
                height: inherit;
                content: '';
                position: absolute;
            }

            &:nth-child(1) {
                width: 215px;
                top: 0;
                right: 0;
            }

            &:nth-child(2) {
                top: 16px;
                left: 0;
                height: 10px;
            }

            &:nth-child(3) {
                top: 42px;
                left: 0;
                height: 10px;
            }
        }
    }

    @keyframes placeholderAnimate {
        0% {
            background-position: -650px 0;
        }
        100% {
            background-position: 650px 0;
        }
    }
    .bbsPlaceLabel {
        text-decoration: none !important;
        border-bottom-width: 1px;
        border-bottom-style: dotted;
        color: inherit;
    }
    .flo_content {
        a {
            text-decoration: underline;
        }
        line-height 26px;
        .bind_place {
            white-space: nowrap;
            line-height 26px;
            a {
                color: #959595;
            }
            a:hover {
                text-decoration underline !important;
            }
            i:before {
                content: '\ea15';
                font-size: 28px;
                vertical-align: -7px;
                color: #959595;
            }
        }
    }
    .load_agian {
        animation: mymove .8s infinite linear;
    }
    .outdata {
        color: #fb6850;
        line-height: 35px;
    }
    .user_info {
        border-radius: 4px;
        position: relative;
        padding: 7px 50px 4px 88px;
        background: #f0f0f0;
        margin-bottom 20px;
        .left {
            position: absolute;
            border: 4px solid #f0f0f0;
            width: 48px;
            height: 48px;
            border-radius: 50%;
            left: 16px;
            top: -14px;
        }
        .user_center {
            margin-left 9px;
            margin-top: -2px;
            .center_top {
                font-size: 14px;
                line-height: 20px;
                color: #636363;
            }
            .center_bottom {
                font-size 12px;
            }
            .name {
                color: #636363;
                font-weight: 700;
                cursor pointer;
            }
            .name:hover {
                color: #10b041
            }
            .auth {
                width: 16px;
                height: 16px;
                vertical-align: -3px;
                margin-left: 3px;
                cursor pointer;
            }
            .lz {
                font-size: 12px !important;
                color: #fff !important;
                background: #c0c0c0;
                border-radius: 8px;
                padding: 2px 5px;
                margin-left: 5px;
                text-align: center;
            }
            .time {
                color: rgba(0, 0, 0, .4);
                line-height 14px;
            }
            .from_app {
                cursor pointer;
                color: #10b041;
                margin-left: 10px;
            }
            .group {
                color: #636363;
                font-size 12px;
                margin-left: 9px;
            }
        }
        .right {
            position: absolute;
            right: 20px;
            top: 19px;
            line-height: 18px;
            color: #636363;
            font-size: 16px;
            cursor pointer;
        }
        .right:hover {
            color: #0cbf79
        }
    }
    .quote {
        font-size 14px;
        margin: 20px 0;
        .left {
            a {
                color: #0073b6;
            }
            .floor {
                color #0CBF79;
            }
            .name {
                margin-left: -4px;
            }
        }
        .right {
            margin-left: 10px;
            color: #959595;
            cursor: pointer;
            .icon {
                font-size 24px;
            }
            .icon:after {
                content '\ea07';
                vertical-align: -7px;
            }
            .isopen:after {
                content '\ea05';
            }
        }
    }
    .warn {
        padding-left: 26px;
        font-size: 14px;
        margin 20px 0;
        color: #fb6850;
    }
    .open_quote {
        margin-bottom 12px;
        font-size 13px !important;
        max-height: 50px;
        display: inline-block;
        .quo_con {
            margin-right: 10px;
            color: #C0C0C0 !important;
        }
        .check_all:before {
            content '\ea41';
            font-size: 19px;
            vertical-align: -5px;
        }
        .check_all {
            margin-top 15px;
            cursor pointer;
        }
    }
    .floor_bot {
        text-align right;
        padding-top: 15px;
        color #0cbf79;
        font-size: 16px;
        span {
            cursor pointer;
        }
        .last_edit {
            font-size: 12px;
            color: #959595;
            float: left;
        }
        .manage_item {
            span {
                margin-left 18px;
            }
            a:hover, span:hover {
                border-bottom: 1px solid #0cbf79;
            }
        }
        img {
            width 12px;
            margin-right: 5px;
        }
        a {
            font-size: 16px;
            margin-right 18px;
            color: #0cbf79;
        }
        .reply_icon:after {
            content: '\EA13';
            font-size: 26px;
            vertical-align: -8px;
            margin-left: 18px;
        }
        .text:hover {
            border-bottom: 1px solid #0cbf79;
        }
    }
    .qyer_video {
        .fontSong {
            display none;
        }
    }
}

