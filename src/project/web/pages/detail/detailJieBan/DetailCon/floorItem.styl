    display: inline-block;
    margin: 0 10px 10px 0;
    padding: 6px 16px;
    border: 1px solid silver;
    border-radius: 3px;
    background-color: #f5f5f5;
    font-size: 14px;
    color: #636363;
    cursor: pointer;
}

.dia_man_btn:hover {
    border-color: #10b041;
    color: #10b041;
    text-decoration: none;
}